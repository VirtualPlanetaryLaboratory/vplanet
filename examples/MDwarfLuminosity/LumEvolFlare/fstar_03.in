sName		fstar_03	                    # Body's name
saModules  stellar flare                        # Modules

# Physical Parameters
dMass		    0.3                                  # Mass, negative -> Earth masses
dAge                 1e6                                 # Assume star is 1 Myr old when disk disperses
dRadGyra         0.5

# FLARE Parameters
dFlareMinEnergy            -1.0e33          # Lower energy of flares in ergs
dFlareMaxEnergy           -1.0e36          # Higher energy of flares in ergs
sFlareFFD                DAVENPORT          #Flare mode to calculate the XUV luminosity. The options are DAVENPORT, LACY and NONE


# STELLAR parameters
sStellarModel                   baraffe           # Stellar evolutin model: `baraffe` or `none`
dSatXUVFrac                        1.e-3           # Saturation level of the XUV luminosity
#dSatXUVTime                   -1

saOutputOrder	Time -Luminosity -Radius Temperature -RotPer -LXUVTot -LXUVStellar -LXUVFlare Age
