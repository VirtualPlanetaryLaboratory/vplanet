sName	          star
saModules	      stellar eqtide
dMass           0.1
dAge            1e6
dRadGyra        0.5        # Radius of gyration (moment of inertia constant)


# EQTIDE Parameters
dTidalTau      -0.01      # Tidal time lag, negative => seconds
dK2            0.5        # Love number of degree 2
sTideModel     t8         # Tidal model, p2=CPL, t8=CTL
saTidePerts    b          # Body name(s) of tidal perturbers

sStellarModel  none
sXUVModel      none
dLuminosity    -1e-3      # Negative => solar units
dSatXUVFrac    1e-3       # With no evolution, L_XUV = dLuminosity * dSatXUVFrac

saOutputOrder   Time -Luminosity -LXUVTot
