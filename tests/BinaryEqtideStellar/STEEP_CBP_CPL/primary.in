# Primary
sName		primary		            # Body's name
saModules stellar binary eqtide

# Physical Parameters
iBodyType       1               # 1 -> Star
dMass		        1.0          		# Mass, negative -> Earth masses
dRotPeriod      -1              # Initial rotation period (- -> d)
dAge		        1.0e6           # Initial age

# EQTIDE Parameters
dTidalQ		     1.0e6 		        # Tidal phase lag
dK2		         0.5 		          # Love number of degree 2
sTideModel	   p2		            # Tidal model, p2=CPL, t8=CTL
dMaxLockDiff   0.01            # Maximum different between Porb, Prot before forcing tidal locking
saTidePerts	   secondary		    # Body name(s) of tidal perturbers

# STELLAR parameters
sStellarModel baraffe           # Stellar evolution model: `baraffe` or `none`
sMagBrakingModel matt           # Magnetic braking model

# Output
saOutputOrder
